{
  "name": "nodejs-sandbox",
  "version": "1.0.0",
  "description": "",
  "source": "src/main.js",
  "main": "build/main.js",
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "node src/prisma/seed.js"
  },
  "scripts": {
    "start": "npm run start:prod",
    "watch:dev": "npm-run-all build:dev serve:dev",
    "start:dev": "nodemon",
    "start:prod": "npm-run-all build serve:prod",
    "serve:prod": "node ./build/bundle.js",
    "serve:dev": "node ./build/main.js",
    "build": "npm run build:prod",
    "build:dev": "npx npm-run-all clean transpile:dev",
    "build:prod": "npx npm-run-all clean transpile:prod",
    "transpile:dev": "webpack --env mode=development",
    "transpile:prod": "webpack --env mode=production",
    "clean": "node clean.js"
  },
  "nodemonConfig": {
    "exec": "npm run watch:dev",
    "watch": [
      "src/*",
      "*.env"
    ],
    "ignore": [
      "build",
      "**/__test__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codesandbox-app/static-template.git"
  },
  "keywords": [],
  "author": "Ives van Hoorne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codesandbox-app/static-template/issues"
  },
  "homepage": "https://github.com/codesandbox-app/static-template#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-flow": "^7.23.3",
    "@types/express": "^4.17.21",
    "babel-loader": "^9.1.3",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jsconfig-paths-webpack-plugin": "^0.1.3",
    "nodemon": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "transpile-webpack-plugin": "^1.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@prisma/client": "5.9.1",
    "body-parser": "^1.20.2",
    "cls-rtracer": "^2.6.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "prisma": "^5.9.1",
    "winston": "^3.11.0"
  }
}
